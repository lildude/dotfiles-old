 = HTTParty.get(\n      \"https://www.strava.com/api/v3/uploads/#{upload['id']}\",\n      headers: { 'Authorization' => \"Bearer #{@token}\" }\n      )\n\n      File.open(\"upload-progress.txt\", \"a\") do |fh|\n        fh.puts(verify.body)\n      end\n      unless verify['error'].nil?\n        puts \"Done. (#{verify['error']})\"\n      else\n        puts \"Done.\"\n      end\n    else\n      puts \"Upload problem: #{upload[\"error\"]}\"\n      puts upload.code\n      exit\n    end\n  rescue Exception => e\n    puts \"Oops, something went wrong: #{e}\"\n    puts upload[\"errors\"]\n    exit\n  end\n\nend\n\ndef determineActivityType(id, name)\n  # Open the #{id}.tcx file\n  File.open(\"#{@tcx_dir}/#{id}.tcx\", 'r') do |fh|\n    fh.find do |line|\n      if line =~ /Activity Sport/\n        act = line.match(/=\"(.*)\"/)[1]\n        if act == \"Running\"\n          act_type = \"run\"\n        elsif act == \"Biking\"\n          act_type = \"ride\"\n        elsif act == \"Other\" and name =~ /swim/i\n          act_type = \"swim\"\n        else\n          act_type = \"workout\"\n        end\n        return act_type\n      end\n    end\n  end\nend\n\ndef start()\n  # Open our mapping file\n  File.open(@mapping, 'r') do |fh|\n    # Iterate over each line\n    fh.each_line do |entry|\n      uploadFile(entry)\n    end\n  end\nend\n\nstart()\n","markers":{"markers":{"1":{"id":1,"range":[[6,0],[9,51]],"tailed":true,"reversed":true,"valid":true,"invalidate":"never","persistent":true,"properties":{"type":"selection","editorId":29,"goalBufferRange":null},"deserializer":"Marker"}},"deserializer":"MarkerManager"},"history":{"undoStack":[],"redoStack":[],"deserializer":"History"},"filePath":"/Users/lildude/Development/garmin-to-strava/up-to-strava.rb","modifiedWhenLastPersisted":false,"digestWhenLastPersisted":"9816511ca19cbfbdc37e005c4594443d504f558b","deserializer":"TextBuffer"},{"text":"#!/usr/bin/env ruby\n#\n# Original taken from http://www.jotlab.com/2013/garmin-2-strava-sync-r